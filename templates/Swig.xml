<templateSet group="Swig">
  <template name="swfor" value="{% for $item$ in $list$ %}&#10;    $END$&#10;{% endfor %}" description="swig for tag" toReformat="true" toShortenFQNames="true">
    <variable name="item" expression="" defaultValue="item" alwaysStopAt="true" />
    <variable name="list" expression="" defaultValue="items" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="swinclude" value="{% include '$template$' %}" description="Includes a template partial in place" toReformat="true" toShortenFQNames="true">
    <variable name="template" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="swblock" value="{% block $blockname$ %}&#10;  $END$&#10;{% endblock %}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="blockname" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="swif" value="{% if $conditions$ %}&#10;$END$&#10;{% endif %}" toReformat="true" toShortenFQNames="true">
    <variable name="conditions" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
</templateSet>