<templateSet group="user">
  <template name="@p" value="@Prop($setting$)&#10;$propName$: $type$" description="@Prop()" toReformat="true" toShortenFQNames="true">
    <variable name="setting" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="propName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_EXPRESSION" value="false" />
      <option name="JS_STATEMENT" value="false" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="*dib" value="display: inline-block;&#10;*display: inline;&#10;zoom: 1;" description="兼容IE 8的display inline-block" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true" />
    </context>
  </template>
  <template name="dvard" value="$name$(state, action) {&#10;  return {&#10;    ...state,&#10;    $END$&#10;  }&#10;}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_EXPRESSION" value="false" />
      <option name="JS_STATEMENT" value="false" />
    </context>
  </template>
  <template name="afn" value="($params$) =&gt; {&#10;    $END$&#10;}" description="() =&gt; {}" toReformat="true" toShortenFQNames="true">
    <variable name="params" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_EXPRESSION" value="true" />
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ssd" value="this.setData($END$)" description="小程序 this.setData()" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="aafn" value="async ($param$) =&gt; {&#10;  $END$&#10;}" description="async () =&gt; {}" toReformat="true" toShortenFQNames="true">
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ES6_EXPRESSION" value="true" />
      <option name="JS_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="todel" value="// TODO: Delete in the product" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ms" value="const mapState = state =&gt; ({&#10;    $END$&#10;})" description="redux mapState" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="mp" value="const mapDispatch = dispatch =&gt; ({&#10;    $END$    &#10;})" description="redux mapDispatch" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ES6_STATEMENT" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>