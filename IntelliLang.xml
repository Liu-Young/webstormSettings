<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JSX Harmony" injector-id="xml">
      <display-name>Header/@avatar</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("avatar")).withParent(xmlTag().withLocalName(string().equalTo("Header")))]]></place>
    </injection>
  </component>
</application>